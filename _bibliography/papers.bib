	doi = {10.1007/s00410-025-02209-5},
	html = {https://doi.org/10.1007/s00410-025-02209-5},
	id = {Wenk2025},
	isbn = {1432-0967},
	journal = {Contributions to Mineralogy and Petrology},
	number = {3},
	pages = {23},
	pdf = {Wenk2025aExploring microstructures and a.pdf},
	preview = {Wenk2025a.png},
	title = {Exploring microstructures and anisotropies of serpentinites},
